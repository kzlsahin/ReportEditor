{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es6",
    "outDir": "wwwroot/js",
    "baseUrl": "wwwroot",
    "paths": {
      "*": [ "*", "/*" ]
    }
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "Areas/ReportEditor/ts/*"
  ]
}
